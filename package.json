{
  "name": "api.iyansr.id",
  "version": "1.0.0",
  "description": "Api for iyansr.id personal web",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iyansr/api.iyansr.id"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/netlify-lambda serve src",
    "build": "NODE_ENV=production ./node_modules/.bin/netlify-lambda build src",
    "test": "NODE_ENV=test jest --testTimeout=10000 --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:coveralls": "NODE_ENV=test jest --coverage && coveralls < coverage/lcov.info"
  },
  "keywords": [
    "Api"
  ],
  "author": "IyanSR",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "marked": "^0.8.1",
    "mongoose": "^5.9.4",
    "netlify-lambda": "^1.6.3",
    "sanitize-html": "^1.22.1",
    "serverless-http": "^2.3.2",
    "shortid": "^2.2.15",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.11",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}